{
  "id": "/votingapp",
  "cmd": null,
  "cpus": 0.2,
  "mem": 128,
  "disk": 0,
  "instances": 2,
  "acceptedResourceRoles": [
    "slave_public"
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "ndemoor/votingapp_voting-app",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "protocol": "tcp",
          "name": "http",
          "labels": {
            "VIP_0": "1.1.1.1:5000"
          }
        }
      ],
      "privileged": false,
      "parameters": [
        {
          "key": "add-host",
          "value": "redis:1.2.3.4"
        }
      ],
      "forcePullImage": false
    }
  },
  "labels": {
    "type": "app"
  }
}
